    "def bubble_sort(li):\n",
    "    n = len(li)\n",
    "    for i in range(n-1):\n",
    "        for j in range(n-i-1):\n",
    "            if li[j] > li[j+1]:\n",
    "                li[j], li[j+1]=li[j+1], li[j]\n",
    "            \n",
    "li = [5, 2, 9, 3]\n",
    "bubble_sort(li)\n",
    "li"
   ]
  }
